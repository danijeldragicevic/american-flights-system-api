#%RAML 1.0 DataType

type: object
properties: 
    ID?: integer
    code: string
    price: number
    departureDate: string
    origin:
        type: string
        minLength: 3
        maxLength: 3
    destination:
        type: string
        enum: ['SFO', 'LAX', 'CLE']
    emptySeats: integer
    plane:
        type: object
        required: false
        properties:
            type: string
            totalSeats: integer
